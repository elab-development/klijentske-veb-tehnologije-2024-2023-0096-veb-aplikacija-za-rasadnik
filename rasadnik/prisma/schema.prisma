generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  password  String
  cart      Json     @default("{}")
  favorites Json     @default("[]")
  plan      Json     @default("{}")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}